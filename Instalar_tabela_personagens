CREATE TABLE characters(
   character_actor   VARCHAR(34) NOT NULL PRIMARY KEY
  ,actor             VARCHAR(27) NOT NULL
  ,episodes_appeared INTEGER  NOT NULL
  ,first_appearance  INTEGER  NOT NULL
  ,last_appearance   INTEGER  NOT NULL
);
